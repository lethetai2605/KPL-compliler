Program Example2; (* Factorial *)
const a= 2.;
      b= 2.3;
Var n : Integer;
    M: DOUBLE;
    s: String;

Function F(n : Integer) : Integer;
  Begin
    If n = 0 Then F := 1 Else F := N * F (N - 1);
  End;

Begin
  For n := 1 To 7 Do
    Begin
      Call WriteLn;
      Call WriteI( F(n));
    End;
  while n=1 DO n:=2;
  DO n:=1 while n<0;
  n,M,s := 3, 3.4,'dad sadas';

End. (* Factorial *)